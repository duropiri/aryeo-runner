{
  "name": "aryeo-delivery-runner",
  "version": "1.0.0",
  "description": "Production service to deliver assets to Aryeo listings via Playwright automation",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "login": "tsx scripts/login-aryeo.ts",
    "export:storage-state": "tsx scripts/export-storage-state.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "bullmq": "^5.12.0",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "playwright": "^1.48.0",
    "tough-cookie": "^6.0.0",
    "undici": "^6.19.8",
    "unzipper": "^0.12.3",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/tough-cookie": "^4.0.5",
    "@types/unzipper": "^0.10.10",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
